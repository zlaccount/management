        //获取文件对象
        var image = obj.files[0];
        //获取文件名
        var name = image.name;
        //文件的大小单位为B
        var size = image.size;
        console.log(size);
        console.log(image.width);
        console.log(image.height);
        //截取出文件后缀
        var suffix = name.substring(name.indexOf("."));
        //图片匹配正则表达式
        var reg = /\.(gif|jpg|jpeg|png|GIF|JPG|PNG)$/;
