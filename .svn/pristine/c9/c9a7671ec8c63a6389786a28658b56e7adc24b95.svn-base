//显示科室下拉列表
function querySubjects(){ 
    $.ajax({
        //192.168.1.51:8333
         url:'http://www.51edoctor.cn/E2306_admin/admin/Information/queryDepartment',
        type:'post',
        data:{
            hospitalId:sessionStorage.getItem("hospitalId"),     
            departmentName:$('#duty_list').val()
        },
        success:function(data){
             if(data[data.length-1] == [] || data[data.length-1].length == 0 ){
                    return false;
                 }
            var list = '';
              for(var i=0;i<data[data.length-1].length;i++){
                 list += '<li>'+data[data.length-1][i].NAME+'</li>';
            }
            $('.list_top .subject_list').slideDown();
            $('.list_top .subject_list').html(list);   
            $('.list_top .subject_list').on('click','li',function click(){
            $('#duty_list').val($(this).text());
                $('.subject_list').slideUp();
                renderList();
            });
            $('#duty_list').on('blur',function(){
                    renderList();
                $('.subject_list').slideUp();
            });     
        },
    });
};
