function query(code,type){
	if(type=="2"){
		document.getElementById("city").options.length=0;
		document.getElementById("country").options.length=0;
		queryCtiy("",code,2);
	}
	if(type=="1"){
		document.getElementById("country").options.length=0;
		queryCtiy("",code,1);
	}
}

