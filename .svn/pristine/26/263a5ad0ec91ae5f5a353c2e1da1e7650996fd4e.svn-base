<script>
    $(function () {
   
     //   dutyMoney();
      // clickDuty();
       // clickHas();
       // clickNon();
        dutyList();
        querySubjectsInput();

        $('.smallMask').hide();
        $('.smallBox').hide();
        $('.small_textInput').hide();
       // $('.btn_subsure').hide();
        // 获取弹窗宽高
        var width=$('.alertBox').width();
        var height=$('.alertBox').height();
        // 获取浏览器窗口宽高
        var h=document.documentElement.clientHeight; 
        var w=document.documentElement.clientWidth;
        var smallW=$('.smallBox').width();
        var smallH=$('.smallBox').height();
        $('.alertBox').css({ 'top': (h-height) / 2 + 'px', 'left': (w-width) / 2 + 'px' });
        $('.alert-close').click(function(){
            $('.alertMask').hide();
            $('.alertBox').hide();
        })
        $('.smallBox').css({ 'top': (h-smallH) / 2 + 'px', 'left': (w-smallW) / 2 + 'px' });
        $('.btn_return').click(function(){
            $('.smallMask').hide();
            $('.smallBox').hide();
        })
    })
    function clearNoNum(obj){
        obj.value = obj.value.replace(/[^\d.]/g,"");  //清除“数字”和“.”以外的字符
        obj.value = obj.value.replace(/\.{2,}/g,"."); //只保留第一个. 清除多余的
        obj.value = obj.value.replace(".","$#$").replace(/\./g,"").replace("$#$",".");
        obj.value = obj.value.replace(/^(\-)*(\d+)\.(\d\d).*$/,'$1$2.$3');//只能输入两个小数
        if(obj.value.indexOf(".")< 0 && obj.value !=""){//以上已经过滤，此处控制的是如果没有小数点，首位不能为类似于 01、02的金额
            obj.value= parseFloat(obj.value);
        }
    }

   
    $(function(){
        $("input").focus(function(){
            $(this).css("background-color","#FFFFCC");
        });
        $("input").blur(function(){
            $(this).css("background-color","#fff");
        });
    });




    
</script>
