
/*弹框*/
/*小弹框*/
.smallMask{
    background: #000;
    position: fixed;
    width: 100%;
    height: 100%;
    opacity: 0.4;
    top: 0;
    left: 0;
    z-index: 10020;
    display: block;
}
.smallBox{
    /*padding: 8px;*/
    width: 300px;
    height: 150px;
    overflow: hidden;
    position: fixed;
    z-index: 10021;
    display: block;
    background-color: #fff;
    border-radius: 10px;
}
.smallBox .alertBox_top{
    height: 40px;
    background-color: #00cdb1;
    padding-left: 20px;
    position: relative;
}

.small-title{
    padding-left: 90px;
    display: inline-block;

    height: 18px;
    font-size: 16px;
    margin-top: 8px;
    color:#fff;
    text-align: center;
}
.small_main{
    text-align: center;
    padding-top: 26px;
}
.small_btn{
    text-align: center;
    margin-top: 27px;
}
.small_btn button{
    background-color: #fff;
    outline: none;
    border: 1px solid #ddd;
    border-radius: 5px;
    width: 60px;
}



