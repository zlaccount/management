	background-color:#d2d2d2;
}
.schedule_head{
    height:80px;
    line-height: 80px;
    padding-left: 30px;
    font-size: 30px;
    border-bottom: 1px solid #ddd;
}
.schedule_search{
    margin-top: 20px;
    padding-left: 30px;
}
.schedule_search input{
    width: 180px;
    height: 33px;
    padding-left: 10px;
    outline: none;
}
input:disabled{
    background-color: #fff !important;
    border:1px solid #D2d2d2 !important;
}
.schedule_search>div{
    float: left;
    margin-right: 30px;
}
.schedule_search button{
    width:137px;
    height: 33px;
    line-height: 29px;
    color: #ffffff;
    border: none;
    background-color: #01cdb2;
    border-radius: 5px;
    outline: none;
}
.schedule_table,.audit_table{
    padding-left: 40px;
    padding-right: 225px;
    /*padding-right: 375px;*/
    margin-top: 20px;
    width: 100%;
    min-width:1700px ;
}
.schedule_name{
    position: relative;
}
.name_suggest{
    background: #fff;
    position: absolute;
    top: 50px;
    left: -1px;
    width: 130px;
    border: 1px solid #dbdbdb;
    display: none;
    z-index: 1;
}
.name_suggest ul{
    padding: 0;
}
.name_suggest ul li{
    padding-left: 10px;
}
.name_suggest ul>li:hover{
    background-color: #f2f2f2;
}

 .schedule_table caption p{
    font-size: 20px;
    font-weight: bold;
    margin: 0;
}
.schedule_table caption span{
    color: #bcbcbc;
}
.schedule_mess img{
    vertical-align: top;
    margin-right: 5px;
}

.schedule_table>div{
    min-height: 686px;
}
.table tbody+tbody{
    border-top:1px solid #ddd ;
}
.schedule_tr_table>tbody>tr>td{
    border-left: 1px solid #ddd;
    border-top:none;
    padding: 8px 0;
}
.schedule_tr_table p{
    padding: 0;
    margin: 0;
}
.schedule_detail_table>tbody>tr>td{
    border-left: 1px solid #ddd;
    border-top:none;
    width:140px;
}
.schedule_detail_table>tbody>tr>td:first-child{
    border-left: none;
}
.schedule_detail_table tr:first-child{
    border-bottom: 1px solid #DDDDDD;
}

tbody>tr.schedule_mess>td{
    border: none;
}
.table-bordered{
    border: none;
}
.mess_left div{
    margin-right: 40px;
}
.mess_right div{
    margin-left: 20px;
}
.mess_right i{
    display: inline-block;
    width:12px;
    height:12px;
    border: 1px solid #ddd;
    vertical-align: middle;
    margin-right: 8px;
}
.has_line i{
    background-color: #2fa5ff;
    border: none;
}
.nervous_line i{
    background-color: #ed6e75;
    border: none;
}
.no_line i{
    background-color: #bababa;
    border: none;
}
.new_schedule{
    color:#01cdb2;
}
.export_table{
    color: #ed6e75;
}
.urgent_make{
    color: #2fa5ff;
}
.modify_record{
    color: #f49809;
}
.schedule_table table>tbody>tr.schedule_tr>td
,.audit_table table>tbody>tr.schedule_tr>td{
    height: 58px;
    padding: 0;
    text-align: center;
    background-color: #f2f2f2;
    vertical-align: middle;
    color: #b2b2b2;
}
.schedule_tr>td>p{
    margin: 0;
}
.point_left{
    position: relative;
}
.point_left a{
    position: absolute;
    display: block;
    width: 9px;
    height: 16px;
    background-image: url("../images/left_03.png");
    top:22px;
    left:10px;
}
.point_right{
    position: relative;
}
.point_right a{
    position: absolute;
    display: block;
    width: 9px;
    height: 16px;
    background-image: url("../images/right_05.png");
    top:22px;
    right:10px;
}
.schedule_detail_up,.schedule_detail_down{
    /*height: 60px;*/
}
.schedule_detail_up p{
    margin: 0;
    padding: 0;
}
.schedule_detail_table>tbody>tr>td{
    padding: 0;
    height: 29px;
    vertical-align: middle;
}
.schedule_time p{
    /*margin-bottom: -1px;*/
    border-bottom: 1px solid #ddd;
    line-height: 33px;
}
.schedule_time p:nth-child(2){
    border-bottom: none;
}
.time_tus{
    border-left:none !important;
}
.point_left{
    border-left:none !important;
}
.table tbody>tr.schedule_detail_up>td,.table tbody>tr.schedule_detail_down>td{
    padding: 0;
    text-align: center;
    vertical-align: middle;
}
