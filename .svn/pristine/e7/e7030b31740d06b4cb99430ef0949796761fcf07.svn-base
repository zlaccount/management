body {
	background-image: url(../images/takeNumber.png);
}

.header {
	position: relative;
	margin: auto auto;
}

.header img {
	position: absolute;
	top: 50px;
	left: 80px;
}

.header a {
	text-decoration: none;
}

.header p {
	font-size: 72px;
	text-align: center;
	color: #fff;
	padding-top: 234px;
}

#head .left_d {
	height: 100px;
	line-height: 100px;
	float: left;
	color: #fff;
	font-size: 54px;
	position: absolute;
	top: 0;
	left: 70px;
}

#head .right_w {
	height: 100px;
	line-height: 160px;
	float: right;
	color: #fff;
	font-size: 36px;
	position: absolute;
	top: 0;
	right: 40px;
}

#foot {
	position: absolute;
	bottom: 20px;
	right: 40px;
}

#foot span {
	font-size: 30px;
	/*color: #fff;*/
}
