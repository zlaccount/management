/*取号弹窗格式*/
.mask {
    position:absolute;
    top:0;
    left:0;
    width:100%;
    height:100%;
    background-color:rgba(0,0,0,.5);
   	/*display:none;*/
}
 .mask .check {
 	background-color: #fff; 
    position:absolute;
    top:50%;
    left:50%;
    transform:translate(-50%,-50%);
    z-index:1;
    width: 290px;
    height: 375px;
 }
.mask .check .title{
	font-size: 32px;
	text-align: center;
    height: 30px;
    line-height: 30px;
    margin-top: 30px
 }
.mask .check .title p {
	margin-top: 10px;
}
.mask .checkMes_box .checkMes {
	margin-top: 60px;
}
