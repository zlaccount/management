/* 分页样式 */
div.page_style{
    padding-right: 0px;
    padding-left: 0px;
    font-size: 14px;
    padding-bottom: 10px;
    color: #333;
    padding-top: 10px;
    font-family: arial, helvetica, sans-serif;
    background-color: #fff;
    text-align: center;
}
div.page_style a {
    border-right: #cecece 1px solid;
    padding-right: 5px;
    border-top: #cecece 1px solid;
    padding-left: 5px;
    padding-bottom: 2px;
    margin: 0px 4px;
    border-left: #cecece 1px solid;
    color: #333;
    padding-top: 2px;
    border-bottom: #cecece 1px solid;
    text-decoration: none;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
}
div.page_style a:hover {
    border-right: #01cdb2 1px solid;
    border-top: #01cdb2 1px solid;
    border-left: #01cdb2 1px solid;
    color: #01cdb2;
    border-bottom: #01cdb2 1px solid;
}
div.page_style span.disabled {
    display: none;
}
div.page_style span.current {
    border-right: #01cdb2 1px solid;
    margin: 0px 4px;
    padding-right: 5px;
    border-top: #01cdb2 1px solid;
    padding-left: 5px;
    font-weight: bold;
    padding-bottom: 2px;
    border-left: #01cdb2 1px solid;
    color: #fff;
    padding-top: 2px;
    border-bottom: #01cdb2 1px solid;
    background-color: #01cdb2;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
}

